    "@mui/material": "^5.12.2",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "1.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router": "^6.11.0",
    "react-router-dom": "^6.11.0",
    "react-toastify": "^9.1.2",
    "redux-persist": "^6.0.0"
